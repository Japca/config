{
  "version": "1.0",
  "logger" : {
    "name" : "git2consul",
    "streams" : [{
      "level": "trace",
      "stream": "process.stdout"
    },
      {
        "level": "debug",
        "type": "rotating-file",
        "path": "/programs/IdeaProjects/my-projects/config/consul/git2consul.log"
      }]
  },

  "repos" : [{
    "name" : "db-service",
    "url" : "https://github.com/Japca/config.git",
    "source_root": "consul",
    "include_branch_name" : false,
    "ignore_file_extension" : true,
    "branches" : ["master"],
    "hooks": [{
      "type" : "polling",
      "interval" : "1"
    }]
  }]
}